.sider
  .inner-menu
     ul 
      //-check xem xem quyền của nó chứa những gì -> cho hiển thị cái đó
      
      li 
        a(href=`${prefixAdmin}/dashboard`) Tổng quan
      
      //-Nếu có quyền view products-category ==> mới hiển thị trên sider
      if(role.permission.includes("products-category_view"))  
        li 
          a(href=`${prefixAdmin}/products-category`) Danh mục Sản phẩm

      if(role.permission.includes("products_view"))      
        li 
          a(href=`${prefixAdmin}/products`) Danh sách Sản phẩm 

      if(role.permission.includes("permission_view"))      
        li 
          a(href=`${prefixAdmin}/roles`) Nhóm quyền

      if(role.permission.includes("role_permission"))        
        li 
          a(href=`${prefixAdmin}/roles/permission`) Phân quyền

      if(role.permission.includes("account_view"))  
        li 
          a(href=`${prefixAdmin}/accounts`) Danh sách tài khoản     
     